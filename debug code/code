		
		if ((args[0].equalsIgnoreCase("sajt")))
		{
			this.registerItem(p.getName()+"_test",p.getItemInHand(),p.getLocation());
			
			p.sendMessage(this.getItemList()[6].getOwner());
			
			this.deregisterItem(this.getItemList()[6].getId());
			
			if(this.getItemList().length<7)
				p.sendMessage("OK");
			
			
		}
